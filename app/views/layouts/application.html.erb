  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
  <% if Rails.env.production? %>
  <%= analytics_init %>
  <% end %>
</head>
<body>
  <div id="wrap">
    <%= render partial: "layouts/header" %>
      
      <%= yield %>
      
    <%= render partial: "layouts/footer" %>
  </div><!-- wrap -->
